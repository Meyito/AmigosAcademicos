<?xml version='1.0' encoding='utf-8'?>
<widget id="co.edu.ufps.amigosacademicos" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <splash density="land-hdpi" src="res/splash-land-hdpi.9.png" />
    <splash density="land-ldpi" src="res/splash-land-ldpi.9.png" />
    <splash density="land-mdpi" src="res/splash-land-mdpi.9.png" />
    <splash density="land-xhdpi" src="res/splash-land-xhdpi.9.png" />
    <allow-intent href="market:*" />
    <name>AmigosAcademicos</name>
    <description>
        Sistema de información para el programa Amigos Académicos de la Universidad Francisco de Paula Santander
    </description>
    <author email="gersonlazaro@gersonlazaro.com" href="https://github.com/Meyito/AmigosAcademicosI/">
        Gerson Lázaro - Melissa Delgado - Daniel Vega
    </author>
    <icon src="res/icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
